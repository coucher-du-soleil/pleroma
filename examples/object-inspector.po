~io
~pleroma.kernel krn

ε InspectorGadget
  - krn.io(io-ctx)

  var io-ctx: io.Io

	δ inspect(object: ε) -> ()
    # Dump attributes
    attr | object.attributes
      # Terminal could be anywhere
      io.main-term() ! print(attr)

      # Local terminal - adds contraints to object/vat
      io.local-term().print(attr)